package lab2.simulate;

import java.util.*;


public class Buffer {
	
	class Packet{
		
		public int sn;
		public int size;
		
		Packet(int sn, int size){
			this.sn = sn;
			this.size = size;
		}
		
		Packet(){
			sn = -1;
			size = -1;
		}
		
	}
	
	LinkedList<Packet> buffer;
	
	public Buffer(int size){
		buffer = new LinkedList<Packet>();
		
		for(int i = 0; i == size; i++){
			buffer.add(new Packet());
		}
		
	}

}
