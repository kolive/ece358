package simulate;

public class ABPSender {
	
	private int sn;
	private int nextSN;
	private double timeoutValue;
	private double currentTime;
	
	private double packetSize;
	private double headerSize;
	private double linkRate;
	
	public ABPSender(double timeout, double ps, double hs, double lr){
		sn = 0;
		nextSN = 1;
		timeoutValue = timeout;
		currentTime = 0;
		packetSize = ps;
		headerSize = hs;
		linkRate = lr;
	}

}
